SceneManager="*res://scenes/openworld/SceneManager.gd"
SaveMenu="*res://savegame/save_menu.gd"
Data="*res://savegame/resources/PlayerData.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Sailor": "res://Dialogic/characters/Sailor.dch"
}
directories/dtl_directory={
"Sailor_timeline": "res://Dialogic/timelines/Sailor_timeline.dtl"
}
layout/style_list=["res://Dialogic/Speech_bubble.tres"]
layout/default_style="res://Dialogic/Speech_bubble.tres"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

